#include <iostream>
using namespace std;
int main(){
return 0;
}
//Multiplicacion
void Multi(float n1,float n2){
    float mult;
    mult=n1*n2;
    cout<<"Resultado de la multiplicacion: "<<mult<<endl;
}
//Division
void Division(float n1, float n2){
    float div;
    div=n1/n2;
    cout<<"Resultado de la division: "<<div<<endl;
}
